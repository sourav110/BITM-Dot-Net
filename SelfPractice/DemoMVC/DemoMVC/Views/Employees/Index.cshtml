@using DemoMVC.Models;
@*@model IEnumerable<DemoMVC.Models.Employee>*@

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
    List<Employee> employees = ViewBag.Employees;
}

<h2>Employee list</h2>

<table class="table">
    <thead>
        <tr>
            <th>SL#</th>
            <th>Name</th>
            <th>Designation</th>
            <th>Age</th>
        </tr>
    </thead>
    <tbody>

        @foreach (Employee employee in employees)
        {
            <tr>
                <td>@employee.EmployeeId</td>
                <td>@employee.Name</td>
                <td>@employee.Designation</td>
                <td>@employee.Age</td>
            </tr>
        }
    </tbody>
</table>

